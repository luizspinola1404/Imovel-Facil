{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "outDir": "server/dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": false,
    "types": ["node"]
  },
  "include": [
    "server/index.ts",
    "server/routes.ts",
    "server/static.ts"
  ],
  "exclude": [
    "server/vite.ts",
    "server/replit_integrations",
    "server/shared",
    "node_modules"
  ]
}
